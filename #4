    "def poly8(x):\n",
    "    p = 0\n",
    "    for i in range(len(POLY8)):\n",
    "        p += POLY8[i]*x**i\n",
    "\n",
    "        \n",
    "    return p\n",
    "\n",
    "\n",
    "def epoly8(x):\n",
    "    p = 0\n",
    "    for i in range(len(EPOLY8)):\n",
    "        p += EPOLY8[i]*x**i\n",
    "\n",
    "        \n",
    "    return p\n",
